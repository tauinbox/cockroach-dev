<div class="article">
  <h1 align="center"><%= @article.title %></h1>
  <div class="panel panel-primary">
    <div class="panel-body">
      <%= @article.body %>
    </div>
    <br/>

    <div class="panel-footer">
      <%= link_to 'Edit', edit_article_path(@article) %> |
      <%= link_to 'Back', articles_path %>
    </div>    
  </div>
</div>